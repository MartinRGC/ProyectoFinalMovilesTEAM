<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IngreseActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="375dp"
        android:layout_height="53dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="48dp"
        android:text="Ingrese sus datos para continuar"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/btnlistusuarios"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/btnlistusuarios"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="23dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:hint="    Nombre"
        android:inputType="text"
        android:textColor="#949494"
        app:layout_constraintBottom_toTopOf="@+id/btnlistrest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/btnlistrest"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="37dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:hint="    Apellido"
        android:inputType="text"
        android:textColor="#949494"
        app:layout_constraintBottom_toTopOf="@+id/btnlisttiendas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlistusuarios"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/btnlisttiendas"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="42dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:hint="    Correo"
        android:inputType="text"
        android:textColor="#949494"
        app:layout_constraintBottom_toTopOf="@+id/btnlistpeddos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlistrest"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/btnlistpeddos"
        android:layout_width="394dp"
        android:layout_height="62dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="46dp"
        android:background="@drawable/rectext1"
        android:ems="10"
        android:hint="    Numero"
        android:inputType="text"
        android:textColor="#949494"
        app:layout_constraintBottom_toTopOf="@+id/btnContnr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlisttiendas"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnContnr"
        android:layout_width="193dp"
        android:layout_height="48dp"
        android:layout_marginBottom="72dp"
        android:background="#0000FF"
        android:text="CONTINUAR"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlistpeddos"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>