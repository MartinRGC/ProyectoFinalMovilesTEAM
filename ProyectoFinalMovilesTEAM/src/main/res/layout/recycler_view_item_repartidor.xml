<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/ciudadRepaTV"
        android:layout_width="82dp"
        android:layout_height="33dp"
        android:layout_weight="1.5"
        android:padding="8dp"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/apellidoRepaTV"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/apellidoRepaTV"
        android:layout_width="82dp"
        android:layout_height="33dp"
        android:layout_weight="1.5"
        android:padding="8dp"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nombreRepartiTV"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nombreRepartiTV"
        android:layout_width="82dp"
        android:layout_height="33dp"
        android:layout_weight="1.5"
        android:padding="8dp"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/numeroRepaTV"
        android:layout_width="82dp"
        android:layout_height="33dp"
        android:layout_weight="1.5"
        android:padding="8dp"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ciudadRepaTV"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/correoRepaTV"
        android:layout_width="82dp"
        android:layout_height="33dp"
        android:layout_weight="1.5"
        android:padding="8dp"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/numeroRepaTV"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>